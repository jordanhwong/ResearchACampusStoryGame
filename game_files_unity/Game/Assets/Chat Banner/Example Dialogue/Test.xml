<dialog>
  <if var="initialized" case="E" value="true">
    <true>
      <page>This was last set to True.</page>
    </true>
    <false>
      <page>This was last set to False.</page>
    </false>
  </if>
  <reply>
    <option>
      <say>Set to True.</say>
	  <set var="initialized" value="true"/>
	</option>
    <option>
      <say>Set to False.</say>
      <set var="initialized" value="false"/>
    </option>
  </reply>
</dialog>